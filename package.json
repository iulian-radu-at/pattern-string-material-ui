		"@types/lodash": "^4.14.118",
		"@types/react": "^16.7.7",
		"@types/react-dom": "^16.0.10",
		"@types/react-select": "^2.0.8",
		"@types/react-transition-group": "^2.0.14",
		"@types/rimraf": "^2.0.2",
		"babel-core": "^6.26.3",
		"babel-jest": "^23.6.0",
		"cross-env": "^5.2.0",
		"enzyme": "^3.7.0",
		"enzyme-adapter-react-16": "^1.7.0",
		"rimraf": "^2.6.2",
		"typescript": "^3.1.6"
	},
	"peerDependencies": {
		"react": "^16.0.0",
		"react-dom": "^16.0.0"
	},
	"jest": {
		"modulePathIgnorePatterns": [
			"./node_modules",
			"./history"
