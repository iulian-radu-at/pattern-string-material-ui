	"description": "A material-ui component which looks like a TEXTFIELD grouping together a dropdown and an input components",
	"main": "lib/index.js",
	"sideEffects": false,
	"scripts": {
		"build": "rimraf ./lib && tsc",
		"deploy": "cross-env NODE_ENV=production npm run build && npm publish",
